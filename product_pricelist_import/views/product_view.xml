<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="product_product_import_tree_view" model="ir.ui.view">
            <field name="name">product.product.import.tree</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field eval="7" name="priority"/>
            <field name="arch" type="xml">
                <field name="lst_price" position="replace"/>
                <field name="attribute_value_ids" position="replace"/>
                <field name="ean13" position="replace"/>
                <field name="uom_id" position="replace"/>
                <field name="name" position="after">
                    <field name="supplier_categ"/>
                </field>
                <field name="name" position="after">
                    <field name="standard_price"/>
                </field>
            </field>
        </record>

        <record id="product_normal_form_import_view" model="ir.ui.view">
            <field name="name">product.normal.form.import.view</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <field name="ean13" position="replace"/>
                <field name="lst_price" position="replace"/>
            </field>
        </record>

    </data>
</openerp>